{
  "name": "readable-types",
  "version": "3.4.0",
  "description": "Utils for typescript language",
  "main": "./dist/index.js",
  "typesVersions": {
    ">=5.0": { "*": ["dist/index.d.ts", "dist/@5.0/index.d.ts"] },
    ">=4.2": { "*": ["dist/index.d.ts"] }
  },
  "scripts": {
    "typeChecking": "bash ./scripts/typeChecking.sh",
    "build": "npx -p typescript@latest tsc -p ./tsconfig.build.json",
    "test": "rtft",
    "debugTypes": "tsc --generateTrace ./outDir",
    "lint": "eslint \"./lib/**/*.{ts,tsx}\" --max-warnings 0",
    "lint:fix": "eslint \"./lib/**/*.{ts,tsx}\" --max-warnings 0 --fix",
    "prepare": "husky install && tsc -p ./rtt-plugin",
    "postversion": "git push && git push --tags --no-verify && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Eythaann/readable-types.git"
  },
  "keywords": [
    "typescript",
    "utility",
    "library",
    "testing",
    "validation",
    "types",
    "interfaces",
    "development",
    "code quality",
    "automation"
  ],
  "author": "eythaann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Eythaann/readable-types/issues"
  },
  "bin": {
    "rtft": "rtt-plugin/dist/run-tests.js"
  },
  "dependencies": {
    "readable-test-for-types": "file:./rtt-plugin",
    "typescript": ">=4.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.0",
    "@commitlint/config-conventional": "^17.4.4",
    "@stylistic/eslint-plugin": "^1.6.3",
    "eslint": "^8.36.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "husky": "^8.0.0",
    "typescript-eslint": "^7.1.1"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "directories": {
    "lib": "lib",
    "dist": "dist"
  }
}
